{
  "name": "service-mw2",
  "version": "1.1.0",
  "description": "Best practices of Midway.js",
  "private": true,
  "dependencies": {
    "@midwayjs/decorator": "^2.11.0",
    "@midwayjs/orm": "^1.3.1",
    "@midwayjs/swagger": "^1.0.7",
    "@midwayjs/web": "^2.11.0",
    "@waiting/egg-jwt": "^6.0.0",
    "@waiting/shared-core": "^12.0.0",
    "@waiting/shared-types": "^12.0.0",
    "bcryptjs": "^2.4.3",
    "egg": "^2.29.4",
    "egg-redis": "^2.4.0",
    "egg-scripts": "^2.13.0",
    "jaeger-client": "^3.18.1",
    "meeko": "^1.8.162",
    "midway": "^2.11.0",
    "midway-component-jaeger": "^6.3.0",
    "midway-component-koid": "^4.8.0",
    "moment": "^2.29.1",
    "mysql2": "^2.2.5",
    "opentracing": "^0.14.5",
    "tsconfig-paths": "^3.9.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@midwayjs/cli": "^1.2.71",
    "@midwayjs/egg-ts-helper": "^1.2.0",
    "@midwayjs/mock": "^2.11.0",
    "@types/jaeger-client": "^3.18.0",
    "@types/jest": "^26.0.23",
    "@types/node": "^15.12.2",
    "@types/rewire": "^2.5.28",
    "commitizen": "^4.2.4",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-unicorn": "^33.0.1",
    "husky": "^6.0.0",
    "jest": "^27.0.4",
    "mwts": "^1.2.2",
    "rewire": "^5.0.0",
    "standard-version": "^9.3.0",
    "swagger-ui-dist": "^3.50.0",
    "ts-jest": "^27.0.3",
    "typescript": "^4.3.2"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "scripts": {
    "start": "egg-scripts start --daemon --title=midway-server-my_midway_project --framework=midway",
    "stop": "egg-scripts stop --title=midway-server-my_midway_project",
    "start_build": "npm run build && cross-env NODE_ENV=development midway-bin dev",
    "dev": "cross-env ets && cross-env NODE_ENV=local midway-bin dev --ts",
    "debug": "cross-env NODE_ENV=local midway-bin dev --debug --ts",
    "test": "midway-bin test --ts --forceExit",
    "cov": "midway-bin cov --ts --forceExit",
    "lint": "mwts check",
    "lint:fix": "mwts fix",
    "ci": "npm run cov",
    "build": "midway-bin build -c",
    "commit": "git-cz",
    "release": "standard-version"
  },
  "midway-bin-clean": [
    ".vscode/.tsbuildinfo",
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/fsd-nodejs/service-mw2"
  },
  "author": "FSD Node.js Group",
  "license": "MIT",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
